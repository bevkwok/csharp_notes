@using practice_viewModel.Models
@model List<User>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Here are some Users</h1>
    <p>
        @foreach (var user in Model)
        {
            <p>
                @user.FirstName @user.LastName
            </p>
        }
    </p>
</body>
</html>