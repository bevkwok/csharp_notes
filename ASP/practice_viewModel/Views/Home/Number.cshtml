@model int[]
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Numbers</title>
</head>
<body>
    <div>
        <h1>Here are some numbers</h1>
            @foreach (var num in Model)
            {
                <p>@num</p>
            }
    </div>
</body>
</html>